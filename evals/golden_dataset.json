  "behavior_tests": [
    {
      "id": "bt_001",
      "input": "I need an ML engineer",
      "expected_action": "generate_jd_or_clarify",
      "expected_tool": null,
      "should_contain": ["engineer"],
      "description": "Initial request should lead to JD or clarification"
    },
    {
      "id": "bt_002",
      "input": "yes",
      "context": "after_jd_presented",
      "expected_action": "search",
      "expected_tool": null,
      "description": "Approval after JD should trigger search"
    },
    {
      "id": "bt_003",
      "input": "contact candidate 1 and 3",
      "context": "after_candidates_shown",
      "expected_action": "draft_email",
      "expected_tool": "send_outreach_email",
      "description": "Selection should trigger email drafting"
    },
    {
      "id": "bt_004",
      "input": "I want more senior candidates",
      "context": "after_candidates_shown",
      "expected_action": "refine_search",
      "expected_tool": null,
      "description": "Refinement request should trigger new search"
    },
    {
      "id": "bt_005",
      "input": "hi",
      "expected_action": "greet_and_ask",
      "expected_tool": null,
      "should_contain": ["how"],
      "description": "Greeting should ask about hiring needs"
    }
  ],
  "e2e_scenarios": [
    {
      "id": "e2e_001",
      "name": "Happy Path",
      "description": "Complete flow from request to email draft",
      "conversation": [
        {"role": "user", "content": "I need a senior Python developer in Dubai"},
        {"role": "assistant", "should_contain": ["python"], "should_call": null},
        {"role": "user", "content": "yes, looks good, search for candidates"},
        {"role": "assistant", "should_contain": ["candidate"], "should_call": null},
        {"role": "user", "content": "contact candidate 1"},
        {"role": "assistant", "should_contain": ["email"], "should_call": "send_outreach_email"}
      ]
    },
    {
      "id": "e2e_002",
      "name": "Refinement Flow",
      "description": "User refines search after seeing candidates",
      "conversation": [
        {"role": "user", "content": "Find me ML engineers, here is the JD: ML Engineer with Python and PyTorch. Now search for candidates."},
        {"role": "assistant", "should_contain": ["candidate"], "should_call": null},
        {"role": "user", "content": "Search again for more senior candidates with 7+ years experience"},
        {"role": "assistant", "should_contain": ["candidate"], "should_call": null}
      ]
    },
    {
      "id": "e2e_003",
      "name": "Clarification Flow",
      "description": "Agent asks for clarification on vague request",
      "conversation": [
        {"role": "user", "content": "I need someone technical"},
        {"role": "assistant", "should_contain": ["what", "role", "skills"], "should_call": null}
      ]
    }
  ]
